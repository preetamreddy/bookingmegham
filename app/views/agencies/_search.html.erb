<%= form_tag agencies_path, :method => "get" do %>
  <%= label_tag :agency %>
  <%= autocomplete_field_tag 'agency', '', autocomplete_agency_name_agencies_path,
    :id_element => "#agencies_search_agency_id" %>
  <%= text_field_tag :id, '', :id => "agencies_search_agency_id", :hidden => true %>
  <%= submit_tag "Go", :name => nil, :class => "button black" %>
<% end %>
