<h1 id="avl_chart_h1">Availability Chart</h1>
<% if session[:consider_blocked_rooms_as_booked] == 'Y' %>
	<%= link_to 'Ignore blocked rooms', 
				availability_chart_path(:include_blocked_rooms => 'N') %>
<% else %>
	<%= link_to 'Include blocked rooms',
				availability_chart_path(:include_blocked_rooms => 'Y') %>
<% end %>

<div id="new_booking"></div>

<div id="chart">
	<%= render 'availability_chart' %>
</div>
